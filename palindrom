public class Palindrom {
    public static void main(String[] args) {
        int n = 121, r = 0, temp = n;

        while(n > 0) {
            r = r * 10 + n % 10;
            n = n / 10;
        }

        if (temp == r)
            System.out.println("Palindrom");
        else
            System.out.println("Not Palindrom");
    }
}

